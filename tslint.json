{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": [
      false
    ],
    "jsx-no-lambda": [
      false
    ],
    "no-debugger": [
      false
    ],
    "interface-over-type-literal": [
      false
    ],
    "member-access": [
      false
    ],
    "interface-name": [
      false
    ],
    "no-shadowed-variable": [
      false
    ],
    "jsx-no-bind": [
      false
    ]
  }
}

